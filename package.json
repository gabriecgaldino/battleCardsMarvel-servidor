  },
  "devDependencies": {
    "@types/node": "^17.0.41",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "@types/express": "^4.17.13",
    "axios": "^0.27.2",
    "cli": "^1.0.1",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "pg": "^8.7.3",
    "tsc": "^2.0.4",
    "typeorm": "^0.3.6",
    "uuidv4": "^6.2.13"
  }
}
